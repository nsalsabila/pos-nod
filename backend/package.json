{
  "name": "nod-pos-backend",
  "version": "0.1.0",
  "description": "NOD POS System Backend - Point of Sales system for machine capsule stores",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "db:migrate": "node dist/database/migrate.js",
    "db:seed": "node dist/database/seed.js"
  },
  "keywords": [
    "pos",
    "point-of-sale",
    "ecommerce",
    "orders",
    "payments"
  ],
  "author": "NOD Team",
  "license": "CONFIDENTIAL",
  "dependencies": {
    "express": "^4.18.2",
    "redis": "^4.6.10",
    "pg": "^8.11.1",
    "prisma": "^5.5.2",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-async-errors": "^3.1.1",
    "joi": "^17.11.0",
    "uuid": "^9.0.1",
    "crypto": "^1.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/jest": "^29.5.11",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^6.0.2"
  }
}
